<h2> Editing Label</h2>
  <%= form_for @label, url:admin_label_path do |f| %>
    <strong> Label</strong>
    <p><%= f.text_field :label, class:"form-control" %></p>
    <%= f.hidden_field :id, :value => "params[:id]" %>
    <%= f.submit 'update label', id:"back" %>
  <% end %>

<script>
	let back = document.getElementById("back")
	back.addEventListener("click", () => {
		window.history.back()
	})
</script>