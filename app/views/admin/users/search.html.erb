<h1>検索結果</h1>

<%= link_to "logout", destroy_admin_session_path, method: :delete %>


<p>検索</p>
  <%= form_tag(admin_search_path,:method => 'get') do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :lastname => nil %>
<% end %>

<ul><% @users.each do |user| %>
  <p><%= link_to "マイページ", admin_users_path(user), class:"user_#{user.id}" %></p>

  <p><%= user.lastname %><%= user.firstname %></p>

  <p><%= user.lastname_kana %><%= user.firstname_kana %></p>

  <p><%= user.phonenumber %></p>

  <p><%= user.email %></p>
</ul>


<% end %>



