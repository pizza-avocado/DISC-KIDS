<%= form_for @artist, url: admin_artists_path do |f| %>

<h1> Artist add・edit</h1>
　　<p> artist name：</p>

　　　　<%= f.text_field :artist %>
　　　　<%= f.hidden_field :id, :value => "params[:id]" %>

　　　　<%= f.submit "add" %>
<%= f.submit 'edit', id:"back" %>
<% end %>



<h1> Artist index</h1>

<% @artists.each do |artist| %>

<p> <%= artist.artist %></p>

<% end %>


<script>
	let back = document.getElementById("back")
	back.addEventListener("click", () => {
		window.history.back()
	})
</script>